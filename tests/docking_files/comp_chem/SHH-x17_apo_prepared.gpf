npts 40 40 40                        # num.grid points in xyz
parameter_file /dls_sw/apps/xchem/autodock/AD4_parameters.dat # force field default parameter file
gridfld SHH-x17_apo_prepared.maps.fld # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C Ca Cl HD N NA OA S Zn # receptor atom types
ligand_types A C N NA                # ligand atom types
receptor SHH-x17_apo_prepared.pdbqt  # macromolecule
gridcenter auto                      # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map SHH-x17_apo_prepared.A.map       # atom-specific affinity map
map SHH-x17_apo_prepared.C.map       # atom-specific affinity map
map SHH-x17_apo_prepared.N.map       # atom-specific affinity map
map SHH-x17_apo_prepared.NA.map      # atom-specific affinity map
elecmap SHH-x17_apo_prepared.e.map   # electrostatic potential map
dsolvmap SHH-x17_apo_prepared.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
