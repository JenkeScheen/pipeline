autodock_parameter_version 4.2         # used by autodock to validate parameter set
outlev 1                               # diagnostic output level
intelec                              # calculate internal electrostatics
seed pid time                          # seeds for random generator
ligand_types A C N NA                  # atoms types in ligand
fld SHH-x17_apo_prepared.maps.fld      # grid_data_file
map SHH-x17_apo_prepared.A.map         # atom-specific affinity map
map SHH-x17_apo_prepared.C.map         # atom-specific affinity map
map SHH-x17_apo_prepared.N.map         # atom-specific affinity map
map SHH-x17_apo_prepared.NA.map        # atom-specific affinity map
elecmap SHH-x17_apo_prepared.e.map     # electrostatics map
desolvmap SHH-x17_apo_prepared.d.map   # desolvation map
move SHH-x17_mol_prepared.pdbqt        # small molecule
about 39.5882 -10.4506 -6.4085         # small molecule center
tran0 39.5882 -10.4506 -6.4085         # initial coordinates/A or random
quaternion0 0.0 0. 0. 1.               # initial orientation
dihe0 0. 0. 0.                         # initial dihedrals (relative) or random
torsdof 3                            # torsional degrees of freedom
rmstol 2.0                             # cluster_tolerance/A
extnrg 1000.0                          # external grid energy
e0max 0.0 10000                        # max initial energy; max number of retries
set_ga
ga_pop_size 150                        # number of individuals in population
sw_max_its 300                         # iterations of Solis & Wets local search
sw_max_succ 4                          # consecutive successes before changing rho
sw_max_fail 4                          # consecutive failures before changing rho
sw_rho 1.0                             # size of local search space to sample
sw_lb_rho 0.01                         # lower bound on rho
ls_search_freq 0.06                    # probability of performing local search on individual
set_psw1                               # set the above pseudo-Solis & Wets parameters
unbound_model extended                 # state of unbound ligand
do_local_only 50                       # do this many LS runs
analysis                               # perform a ranked cluster analysis
